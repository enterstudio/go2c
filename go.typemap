# A Perl typemap file for Go exported types

TYPEMAP
GoInt		T_INT
GoUint		T_U_INT
GoInt8		T_CHAR
GoUint8		T_U_CHAR
GoInt16		T_SHORT
GoUint16	T_U_SHORT
GoInt32		T_INT
GoUint32	T_U_INT
GoInt64		LONG_LONG_INT
GoUint64	LONG_LONG_U_INT
GoFloat32	T_FLOAT
GoFloat64	T_DOUBLE

INPUT
LONG_LONG_INT
	$var = (long long )SvIV($arg);
LONG_LONG_U_INT
	$var = (unsigned long long )SvIV($arg);

OUTPUT
LONG_LONG_INT
	sv_setiv($arg, (IV)$var);
LONG_LONG_U_INT
	sv_setiv($arg, (IV)$var);
